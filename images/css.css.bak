@charset "utf-8";
/* CSS Document */
* {
	margin:0;
	padding:0;
}
body {
	font-family:"宋体", Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#000;
	background:#fff url(bg.jpg) repeat-x;
	-webkit-text-size-adjust:none;
	behavior:url("csshover.htc");
}
a {
	font-size:12px;
	color:#000;
	text-decoration:none;
}
a:hover {
	text-decoration:underline;
	color:#ff6600;
}
ul {
	list-style:none;
}
.fl {
	float:left;
}
.fr {
	float:right;
}
img {
	border:none;
}
.clear {
	height:1px;
	clear:both;
	margin-bottom:-1px;
	overflow:hidden;
}
.hidden {
	display:none;
}
.fc, .fc a {
	color:#fb0006;
}
.fc2, .fc2 a {
	color:#0082fb;
}
.fc3, .fc3 a {
	color:#724b97;
}
.fc4, .fc4 a {
	color:#e12121;
}
.fc5 {
	color:#e12121;
	font-size:16px;
}
.fc6 {
	color:#f8a20d;
	font-size:16px;
}
.fc7 {
	color:#0052db;
	font-size:14px;
}
.fc8,.fc8 a{
	color:#f68b00;
}

/*头部*/
#index_head {
	width:1002px;
	margin:0 auto;
	position:relative;
	z-index:9999;
}
#index_logo {
	width:223px;
	height:115px;
	float:left;
	color:#fff;
	position:relative;
	padding:32px 0 0 16px;
}
#index_logor {
	width:447px;
	float:right;
	background:url(top_bg.jpg) no-repeat;
}
#index_logor .topul {
	width:387px;
	padding-left:60px;
	height:62px;
}
#index_logor .topul li {
	width:81px;
	height:32px;
	line-height:32px;
	text-indent:25px;
	position:relative;
	float:left;
}
#index_logor .topul li .li_img {
	position:absolute;
	top:0;
	left:0;
	text-indent:0;
}
#index_logor .topul li.li2 {
