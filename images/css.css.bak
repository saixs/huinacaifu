	font-size:18px;
}
#index_logor .index_tel p.p2 {
	padding-left:29px;
	color:#401b68;
	font-size:24px;
}
#index_nav {
	width:1002px;
	height:45px;
	position:relative;
}
#index_nav li {
	width:139px;
	height:45px;
	line-height:45px;
	float:left;
	color:#fff;
	font-family:"微软雅黑";
	background:url(nav_line.jpg) no-repeat left bottom;
	text-align:center;
	font-size:16px;
	cursor:pointer;
	position:relative;
}
#index_nav li a {
	height:45px;
	display:block;
	color:#fff;
	font-size:16px;
}
#index_nav li:hover {
	background:url(nav_a2.jpg) no-repeat left top;
}
#index_nav li a:hover {
	background:url(nav_a2.jpg) no-repeat left top;
	color:#fff;
	text-decoration:none;
}
#index_nav li #a {
	/*display:none;*/
	position:absolute;
	top:45px;
	left:12px;
	z-index:9999;
	width:116px;
	display:none;
}
#index_nav li #a dd {
	width:116px;
	height:33px;
	line-height:32px;
	text-align:left;
	text-indent:39px;
	background:url(nav_a3.png) no-repeat;
}
#index_nav li #a dd a {
	width:116px;
	height:33px;
	line-height:32px;
	font-size:12px;
	font-family:"宋体";
	font-weight:normal;
	color:#fff;
}
#index_nav li #a dd a:hover {
	background:url(nav_a4.png) no-repeat;
}
#index_nav li:hover #a {
	display:block;
}
#index_nav li.li2 {
	background:none;
}
#index_nav li.li2 a:hover {
	background:url(nav_a.jpg) no-repeat 12px top;
}
#index_nav li.li3 #a a {
	background:none;
}
#index_nav li.li4 #a a {
	background:none;
}
#index_nav li.li3 a {
	background:url(nav_a5.png) no-repeat left top;
}
#index_nav li.li4 a {
	background:url(nav_a.jpg) no-repeat 12px top;
}
#index_main {
	width:100%;
